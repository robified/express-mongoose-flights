<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
  
<body>
  <table id="list">
    <h1><%= title %></h1>
    <thead>
      <tr>
        <th>Airline</th>
        <th>Flight<br>Number</th>
        <th>Departure<br>Time</th>
      </tr>
    </thead>

    <tbody>
      <% flights.forEach(function(f) { %>
      <tr>
        <td><%= f.airline %></td>
        <td><%= f.flightNo %></td>
        <td><%= f.departs %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>

</body>
</html>
