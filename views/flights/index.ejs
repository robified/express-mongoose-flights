<%- include('../partials/header') %>

<table id="list">
    <thead>
      <tr>
        <th>Airline</th>
        <th>Flight<br>Number</th>
        <th>Departure<br>Time</th>
        <th>Flight Details</th>
      </tr>
    </thead>

    <tbody>
      <% flights.forEach(function(f) { %>
      <tr>
        <td><%= f.airline %></td>
        <td><%= f.flightNo %></td>
        <td><%= f.departs %></td>
        <td><a href="/flights/<%= f.id %>">Details</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>

  <%- include('../partials/footer') %>